void e2Test(void)
{

    /* EEPROM SETTINGS */
    SysCtlPeripheralEnable(SYSCTL_PERIPH_EEPROM0); // EEPROM activate
    EEPROMInit(); // EEPROM start TODO: Verify return code

    //Write struct to EEPROM start from 0x0000
    EEPROMProgram((uint32_t *) &ptgmSettingsWrite, E2PROM_TEST_ADRES,
                  sizeof(ptgmSettingsWrite));

    //Read from struct at EEPROM start from 0x0000
    EEPROMRead((uint32_t *) &ptgmSettingsRead, E2PROM_TEST_ADRES,
               sizeof(ptgmSettingsRead));

    ptgmSettingsWrite.channel[15].channel_purpose='v';

    //Write struct to EEPROM start from 0x0000
    EEPROMProgram((uint32_t *) &ptgmSettingsWrite, E2PROM_TEST_ADRES,
                  sizeof(ptgmSettingsWrite));

    //Read from struct at EEPROM start from 0x0000
    EEPROMRead((uint32_t *) &ptgmSettingsRead, E2PROM_TEST_ADRES,
               sizeof(ptgmSettingsRead));


    System_printf("Here");

}
